{
  "name": "immutable-sorted-map",
  "version": "0.2.1",
  "description": "SortedMap implemented in Immutable.js",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf lib",
    "build": "webpack",
    "lint": "standard | snazzy",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rongierlach/immutable-sorted-map.git"
  },
  "keywords": [
    "immutable",
    "immutablejs",
    "sorted",
    "map",
    "keys"
  ],
  "author": "https://github.com/rongierlach",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/rongierlach/immutable-sorted-map/issues"
  },
  "homepage": "https://github.com/rongierlach/immutable-sorted-map#readme",
  "peerDependencies": {
    "extendable-immutable": "^1.3.2",
    "immutable": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "commitizen": "^2.9.6",
    "coveralls": "^2.13.1",
    "cz-conventional-changelog": "^2.0.0",
    "extendable-immutable": "^1.3.2",
    "immutable": "4.0.0-rc.2",
    "jest": "^20.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "webpack": "^2.5.1",
    "webpack-make-rule": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "it",
      "expect",
      "describe"
    ]
  },
  "jest": {
    "verbose": true,
    "coverageDirectory": "coverage"
  }
}
